{
    "compilerOptions": {
        "baseUrl": "./",
        "noEmit": true,
        "module": "esnext",
        "target": "esnext",
        "allowJs": true,
        "moduleResolution": "bundler",
        "sourceMap": true,
        "strict": true,
        "allowUnreachableCode": false,
        "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/ts/"
    ]
}
